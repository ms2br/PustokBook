@model PaginatonVM<IEnumerable<AdminProductImageListItemVM>>
@section styles {
    <!-- Custom fonts for this template-->
    <link href="~/admin/vendor/fontawesome-free/css/all.min.css" rel="stylesheet" type="text/css">
    <link href="https://fonts.googleapis.com/css?family=Nunito:200,200i,300,300i,400,400i,600,600i,700,700i,800,800i,900,900i"
          rel="stylesheet">
    <!-- Custom styles for this template-->
    <link href="~/admin/css/sb-admin-2.min.css" rel="stylesheet">
}
@section scripts {
    <script src="https://code.jquery.com/jquery-3.5.1.min.js"></script>
    <script src="~/admin/js/sb-admin-2.min.js"></script>
    <script>
        $("#pagination").on("click","#prod-pag a", function (ev) {
            ev.preventDefault();
            var url = $(this).attr("href");
            $.ajax({
                url: url,
                method: "GET",
                success: function (data) {
                    $("#pagination").html(data);
                }
            });
        });
    </script>
}
<a asp-action="Create" class="btn btn-success m-3">Create</a>
<div id="pagination">
    <partial name="_ProductImagePaginationPartial" model="Model" />  
</div>

